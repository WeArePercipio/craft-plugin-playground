{%- extends "_boilerplate/_layouts/generic-page-layout.twig" -%}

{%- block headLinks -%}
    {{ parent() }}
    <link rel="amphtml" href="{{- craft.app.request.absoluteUrl ~ '/amp' -}}">
{%- endblock headLinks -%}

{%- block content -%}
        {%- switch entry.type -%}

                {%- case "landingsPage" -%}

                    {%- include '_organisms/entry-types/page--landing' ignore missing with {
                        content: entry,
                    } -%}

        {%- endswitch -%}
{%- endblock -%}

{# -- Any JavaScript that should be included before </body> -- #}
{%- block bodyJs -%}
    {{ parent() }}
{%- endblock bodyJs -%}
